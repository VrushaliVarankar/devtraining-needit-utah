        <script><![CDATA[var IncidentPriorityCheck = Class.create();
IncidentPriorityCheck.prototype = {
    initialize: function() {},

    isHighPriority: function(incidentId) {
        var gr = new GlideRecord('incident');
        if (gr.get(incidentId)) {
            return gr.priority == 1;
        } else {
            return false;
        }
    }
};]]></script>
